{
    "name": "nexusstudio/attributerouter-pro",
    "description": "Modern PHP 8.1+ router using Attributes. Zero dependencies, production-ready, high-performance.",
    "type": "library",
    "keywords": ["php", "router", "attributes", "php8", "routing", "middleware", "rest-api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nexus Studio",
            "email": "nexusstudio100@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AttributeRouter\\": "src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AttributeRouter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "scan": "php router.php scan"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}